@page
@model PipeBuild.Pages.ProjectsModel
@{
    ViewData["Title"] = "Our Projects";
}

<section id="projects_one">
    <div class="container-fluid background-bg bg-opacity-50 main-contacts position-relative">
        <div class="bg-greyed-out bg-grey-pos bg-fixed">
            <div class="w-100 h-100 bg-dark bg-opacity-50"></div>
        </div>
        <div class="container"><h1 class="main-header">@ViewData["Title"]</h1></div>
    </div>
</section>

<section id="projects_three">
    <div class="container section-projects">
        <div class="options mt-lg-4 mt-md-2 mt-sm-2 mb-lg-4 mb-md-2 mb-sm-2 ">
            <ul id="project-filter">
                <li><a data-filter="*" >All</a></li>
                <li><a data-filter=".residential">Residentials</a></li>
                <li><a data-filter=".commercial">Commercials</a></li>
                <li><a data-filter=".industrial">Industrial</a></li>
                <li><a data-filter=".mixeduse">Mixed-Use</a></li>
                <li><a data-filter=".hotel">Hotels</a></li>
            </ul>
        </div>
        <div class="row portfolio-grid text-style">
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 grid-item residential" data-category="residential">
                <div class="portfolio-item">
                    <div class="pt-img"><img src="http://via.placeholder.com/370x520" alt=""></div>
                    <div class="item-info">
                        <span>Residentials</span>
                        <h3><a href="#" title="">UMI Complex Private Residence</a></h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-8 col-md-8 col-sm-6 col-xs-12 grid-item industrial" data-category="industrial">
                <div class="portfolio-item">
                    <div class="pt-img"><img src="http://via.placeholder.com/770x520" alt=""></div>
                    <div class="item-info">
                        <span>Industrial</span>
                        <h3><a href="#" title="">UMI Complex Private Residence</a></h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12 grid-item mixeduse" data-category="mixeduse">
                <div class="portfolio-item">
                    <div class="pt-img"><img src="http://via.placeholder.com/1170x520" alt=""></div>
                    <div class="item-info">
                        <span>Mixed-Use Development</span>
                        <h3><a href="#" title="">MME Consulting Office</a></h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-8 col-md-8 col-sm-6 col-xs-12 grid-item hotel" data-category="hotel">
                <div class="portfolio-item">
                    <div class="pt-img"><img src="http://via.placeholder.com/770x520" alt=""></div>
                    <div class="item-info">
                        <span>Hotels</span>
                        <h3>
                            <a href="#" title="">Heydar Aliyev Center in Baku, Azerbaijan</a>
                        </h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 grid-item hotel" data-category="hotel">
                <div class="portfolio-item">
                    <div class="pt-img"><img src="http://via.placeholder.com/370x520" alt=""></div>
                    <div class="item-info">
                        <span>Hotels</span>
                        <h3><a href="#" title="">Up-Co Working Office Building</a></h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 grid-item mixeduse" data-category="mixeduse">
                <div class="portfolio-item">
                    <div class="pt-img"><img src="http://via.placeholder.com/370x520" alt=""></div>
                    <div class="item-info">
                        <span>Mixed-Use Development</span>
                        <h3><a href="#" title="">Art & Culture Museum In Barcelona</a></h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 grid-item industrial" data-category="industrial">
                <div class="portfolio-item">
                    <div class="pt-img"><img src="http://via.placeholder.com/370x520" alt=""></div>
                    <div class="item-info">
                        <span>Industrial</span>
                        <h3>
                            <a href="#" title="">Beijing Nation Stadium In Beijing, China</a>
                        </h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 grid-item commercial" data-category="commercial">
                <div class="portfolio-item">
                    <div class="pt-img"><img src="http://via.placeholder.com/370x520" alt=""></div>
                    <div class="item-info">
                        <span>Commercials</span>
                        <h3><a href="#" title="">Alex’s Villa</a></h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 grid-item residential" data-category="residential">
                <div class="portfolio-item">
                    <div class="pt-img">
                        <img src="http://via.placeholder.com/370x520" alt="">
                    </div>
                    <div class="item-info">
                        <span>Residentials</span>
                        <h3><a href="#" title="">Alex’s Villa</a></h3>
                    </div>
                </div>
            </div>
            <div class="col-lg-8 col-md-8 col-sm-6 col-xs-12 grid-item industrial" data-category="industrial">
                <div class="portfolio-item">
                    <div class="pt-img"><img src="http://via.placeholder.com/770x520" alt=""></div>
                    <div class="item-info">
                        <span>Industrial</span>
                        <h3><a href="#" title="">UMI Complex Private Residence</a></h3>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="load-more-items">
        <a href="#" title="">Load more</a>
    </div><!--load-more-items end-->

</section>