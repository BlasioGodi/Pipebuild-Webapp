<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Title -->
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="Content-Security-Policy" content="<directive><value>;<directive><value>" />
    <meta http-equiv="cache-control" content="max-age=31536000, public" />
    <meta name="viewport" content="width=device-width,initial-scale=1" />
    <title>PipeBuild | @ViewData["Title"] </title>
    <!--MetaData-->
    <meta name="description"
          content="We are seasoned Gold Forex Traders helping individuals and institutions meet and surpass their financial goals." />
    <meta name="theme-color" content="#f89a4b" />
    <link rel="canonical" href="https://pipebuildkenya.com/" />
    <meta property="og:locale" content="en_US" />
    <meta property="og:type" content="Website" />
    <meta property="og:title" content="PipeBuild" />
    <meta property="og:description"
          content="We are seasoned Gold Forex Traders helping individuals and institutions meet and surpass their financial goals." />
    <meta property="og:url" content="https://pipebuildkenya.com/" />
    <meta property="og:site_name" content="PipeBuild" />
    <meta property="og:image" content="https://pipebuildkenya.com/src/img/Logo.webp" />
    <meta property="og:image:width" content="1280" />
    <meta property="og:image:height" content="720" />
    <meta name="keywords"
          content="Plumbing Installations, Drainage Installations, Swimming Pools, Plumbing Repairs, LPG Gas, LPG Storage" />
    <meta name="author" content="PipeBuild" />

    <!-- Favicons -->
    <link rel="shortcut icon" sizes="64x64" width="64px" height="64px" href="~/src/logos/XAU-64x64.webp" />
    <link rel="apple-touch-icon" sizes="32x32" width="32px" height="32px" href="~/src/logos/XAU-32x32.webp" />
    <link rel="apple-touch-icon" sizes="72x72" width="72px" height="72px" href="~/src/logos/XAU-72x72.webp" />
    <link rel="apple-touch-icon" sizes="114x114" width="114px" height="114px" href="~/src/logos/XAU-114x114.webp" />

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Anton&family=Domine:wght@400;500;600;700&display=swap" rel="stylesheet">

    <link rel="stylesheet" href="~/css/bootstrap_custom.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/PipeBuild.styles.css" asp-append-version="true" />

    <!-- Revolution Style Sheets -->
    <link rel="stylesheet" type="text/css" href="~/lib/rev-slider/revolution/css/settings.css">
    <!-- Revolution Layer Styles -->
    <link rel="stylesheet" type="text/css" href="~/lib/rev-slider/revolution/css/layers.css">
    <!-- Revolution Navigation Styles -->
    <link rel="stylesheet" type="text/css" href="~/lib/rev-slider/revolution/css/navigation.css">

</head>
<body id="page-top" data-spy="scroll" data-target=".navbar" data-offset="100">
    @await Html.PartialAsync("_Header")
    <div class="layout">
        <main role="main">
            @RenderBody()
        </main>
    </div>
    @await Html.PartialAsync("_Footer")

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/node_modules/bootstrap/dist/js/bootstrap.bundle.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <!-- Load JQUERY library -->
    <script type="text/javascript" src="https://code.jquery.com/jquery-3.6.4.js"></script>
    <!-- Revolution JS Files -->
    <script type="text/javascript" src="~/lib/rev-slider/revolution/js/jquery.themepunch.tools.min.js"></script>
    <script type="text/javascript"
            src="~/lib/rev-slider/revolution/js/jquery.themepunch.revolution.min.js"></script>
    <!-- Slider Revolution 5.O Extensions  (Load Extensions only on Local File Systems !  The following part can be removed on Server for On Demand Loading) -->
    <script type="text/javascript"
            src="~/lib/rev-slider/revolution/js/extensions/revolution.extension.actions.min.js"></script>
    <script type="text/javascript"
            src="~/lib/rev-slider/revolution/js/extensions/revolution.extension.carousel.min.js"></script>
    <script type="text/javascript"
            src="~/lib/rev-slider/revolution/js/extensions/revolution.extension.kenburn.min.js"></script>
    <script type="text/javascript"
            src="~/lib/rev-slider/revolution/js/extensions/revolution.extension.layeranimation.min.js"></script>
    <script type="text/javascript"
            src="~/lib/rev-slider/revolution/js/extensions/revolution.extension.migration.min.js"></script>
    <script type="text/javascript"
            src="~/lib/rev-slider/revolution/js/extensions/revolution.extension.navigation.min.js"></script>
    <script type="text/javascript"
            src="~/lib/rev-slider/revolution/js/extensions/revolution.extension.parallax.min.js"></script>
    <script type="text/javascript"
            src="~/lib/rev-slider/revolution/js/extensions/revolution.extension.slideanims.min.js"></script>
    <script type="text/javascript"
            src="~/lib/rev-slider/revolution/js/extensions/revolution.extension.video.min.js"></script>
    <script type="text/javascript" src="~/js/revolution.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>